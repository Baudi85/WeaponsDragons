        "blink": "controller.animation.persona.blink"
      },
      "render_controllers": [
        { "controller.render.player.first_person": "variable.is_first_person" },
        { "controller.render.player.third_person": "!variable.is_first_person && !variable.map_face_icon" },
        { "controller.render.player.map": "variable.map_face_icon" },

        { "controller.render.player.bronze_axe": "variable.bronze_axe" },
        { "controller.render.player.curved_sword": "variable.curved_sword" },
        { "controller.render.player.dark_blade": "variable.dark_blade" },
        { "controller.render.player.double_axe": "variable.double_axe" },
