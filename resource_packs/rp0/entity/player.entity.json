{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:player",
      "materials": {
        "default": "entity_alphatest",
        "cape": "entity_alphatest",
        "animated": "player_animated"
      },
      "textures": {
        "default": "textures/entity/steve",
        "cape": "textures/entity/cape_invisible",
        "bronze_axe": "textures/items/models/bronze_axe",
        "curved_sword": "textures/items/models/curved_sword",
        "dark_blade": "textures/items/models/dark_blade",
        "double_axe": "textures/items/models/double_axe",
        "emerald_broadsword": "textures/items/models/emerald_broadsword",
        "ice_spear": "textures/items/models/ice_spear",
        "nimble_dagger": "textures/items/models/nimble_dagger",
        "royal_sword": "textures/items/models/royal_sword",
        "valor_hammer": "textures/items/models/valor_hammer",
        "vanguard_sword": "textures/items/models/vanguard_sword",
        "dragon_armor": "textures/items/models/dragon_armor"
      },
      "geometry": {
        "default": "geometry.humanoid.custom",
        "cape": "geometry.cape",
        "bronze_axe": "geometry.bronze_axe",
        "curved_sword": "geometry.curved_sword",
        "dark_blade": "geometry.dark_blade",
        "double_axe": "geometry.double_axe",
        "emerald_broadsword": "geometry.emerald_broadsword",
        "ice_spear": "geometry.ice_spear",
        "nimble_dagger": "geometry.nimble_dagger",
        "royal_sword": "geometry.royal_sword",
        "valor_hammer": "geometry.valor_hammer",
        "vanguard_sword": "geometry.vanguard_sword",
        "player_dragon_armor": "geometry.player_dragon_armor"
      },
      "scripts": {
        "scale": "0.9375",
        "initialize": [

          "variable.bronze_axe = 0;",
          "variable.curved_sword = 0;",
          "variable.dark_blade = 0;",
          "variable.double_axe = 0;",
          "variable.emerald_broadsword = 0;",
          "variable.ice_spear = 0;",
          "variable.nimble_dagger = 0;",
          "variable.royal_sword = 0;",
          "variable.valor_hammer = 0;",
          "variable.vanguard_sword = 0;",
          "variable.sword = 0;",
          
          "variable.dragon_armor_helmet = 0;",
          "variable.dragon_armor_chestplate = 0;",
          "variable.dragon_armor_leggings = 0;",
          "variable.dragon_armor_boots = 0;",


          "variable.is_holding_right = 0.0;",
          "variable.is_blinking = 0.0;",
          "variable.last_blink_time = 0.0;",
          "variable.hand_bob = 0.0;"
        ],
        "pre_animation": [
          
          "variable.bronze_axe = (query.get_equipped_item_name('main_hand') == 'bronze_axe');",
          "variable.curved_sword = (query.get_equipped_item_name('main_hand') == 'curved_sword');",
          "variable.dark_blade = (query.get_equipped_item_name('main_hand') == 'dark_blade');",
          "variable.double_axe = (query.get_equipped_item_name('main_hand') == 'double_axe');",
          "variable.emerald_broadsword = (query.get_equipped_item_name('main_hand') == 'emerald_broadsword');",
          "variable.ice_spear = (query.get_equipped_item_name('main_hand') == 'ice_spear');",
          "variable.nimble_dagger = (query.get_equipped_item_name('main_hand') == 'nimble_dagger');",
          "variable.royal_sword = (query.get_equipped_item_name('main_hand') == 'royal_sword');",
          "variable.valor_hammer = (query.get_equipped_item_name('main_hand') == 'valor_hammer');",
          "variable.vanguard_sword = (query.get_equipped_item_name('main_hand') == 'vanguard_sword');",

          "variable.sword = (variable.curved_sword || variable.dark_blade || variable.emerald_broadsword || variable.ice_spear || variable.nimble_dagger || variable.royal_sword || variable.vanguard_sword);",

          // "variable.dragon_armor_helmet = query.is_stunned && query.is_item_name_any('slot.armor.head',0,'minecraft:netherite_helmet');",
          // "variable.dragon_armor_chestplate = query.is_sheared && query.is_item_name_any('slot.armor.chest',0,'minecraft:netherite_chestplate');",
          // "variable.dragon_armor_leggings = query.is_saddled && query.is_item_name_any('slot.armor.legs',0,'minecraft:netherite_leggings');",
          // "variable.dragon_armor_boots = query.is_tamed && query.is_item_name_any('slot.armor.feet',0,'minecraft:netherite_boots');",

          "variable.dragon_armor_helmet = query.is_stunned;",
          "variable.dragon_armor_chestplate = query.is_sheared;",
          "variable.dragon_armor_leggings = query.is_saddled;",
          "variable.dragon_armor_boots = query.is_tamed;",

          "variable.helmet_layer_visible = 1.0;",
          "variable.leg_layer_visible = 1.0;",
          "variable.boot_layer_visible = 1.0;",
          "variable.chest_layer_visible = 1.0;",
          "variable.attack_body_rot_y = Math.sin(360*Math.sqrt(variable.attack_time)) * 5.0;",
          "variable.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.first_person_rotation_factor = math.sin((1 - variable.attack_time) * 180.0);",
          "variable.hand_bob = query.life_time < 0.01 ? 0.0 : variable.hand_bob + ((query.is_on_ground && query.is_alive ? math.clamp(math.sqrt(math.pow(query.position_delta(0), 2.0) + math.pow(query.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - variable.hand_bob) * 0.02;",

          "variable.map_angle = math.clamp(1 - variable.player_x_rotation / 45.1, 0.0, 1.0);",
          "variable.item_use_normalized = query.main_hand_item_use_duration / query.main_hand_item_max_duration;"
        ],
        "animate": [
          "root",

          {"bronze_axe":"variable.bronze_axe && !variable.is_first_person"},
          {"first_person_bronze_axe":"variable.bronze_axe && variable.is_first_person"},
          {"curved_sword":"variable.sword && !variable.is_first_person"},
          {"first_person_curved_sword":"variable.sword && variable.is_first_person"},
          // {"dark_blade":"variable.dark_blade && !variable.is_first_person"},
          // {"first_person_dark_blade":"variable.dark_blade && variable.is_first_person"},
          {"double_axe":"variable.double_axe && !variable.is_first_person"},
          {"first_person_double_axe":"variable.double_axe && variable.is_first_person"},
          // {"emerald_broadsword":"variable.emerald_broadsword && !variable.is_first_person"},
          // {"first_person_emerald_broadsword":"variable.emerald_broadsword && variable.is_first_person"},
          // {"ice_spear":"variable.ice_spear && !variable.is_first_person"},
          // {"first_person_ice_spear":"variable.ice_spear && variable.is_first_person"},
          // {"nimble_dagger":"variable.nimble_dagger && !variable.is_first_person"},
          // {"first_person_nimble_dagger":"variable.nimble_dagger && variable.is_first_person"},
          // {"royal_sword":"variable.royal_sword && !variable.is_first_person"},
          // {"first_person_royal_sword":"variable.royal_sword && variable.is_first_person"},
          {"valor_hammer":"variable.valor_hammer && !variable.is_first_person"},
          {"first_person_valor_hammer":"variable.valor_hammer && variable.is_first_person"},
          // {"vanguard_sword":"variable.vanguard_sword && !variable.is_first_person"},
          // {"first_person_vanguard_sword":"variable.vanguard_sword && variable.is_first_person"},

          {"first_person_dragon_armor":"variable.is_first_person"}

          // {"hide_item":"variable.sword || variable.bronze_axe || variable.double_axe || variable.valor_hammer"}
          // {"first_person_hide_item":"variable.is_first_person && (variable.sword || variable.bronze_axe || variable.double_axe || variable.valor_hammer)"}
        ]
      },
      "animations": {
        // "hide_item":"animation.player.custom_weapon.hide_item",
        // "first_person_hide_item":"animation.player.custom_weapon.first_person.hide_item",
        "bronze_axe": "animation.player.bronze_axe.hold",
